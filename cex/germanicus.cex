urn:cts:latinLit:phi0881.phi003.breysig_dipl:676#expulit et celsis ophiuchus fulget in astris.
urn:cts:latinLit:phi0881.phi003.breysig_dipl:677#nil trahit obscurum serpens et trunca recepta
urn:cts:latinLit:phi0881.phi003.breysig_dipl:678#desinit esse manu membris deformis imago.
urn:cts:latinLit:phi0881.phi003.breysig_dipl:679#iam lyra cum superis et cygni dextera penna,
urn:cts:latinLit:phi0881.phi003.breysig_dipl:680#radit tellurem rediens cum sidere Cepheus.
urn:cts:latinLit:phi0881.phi003.breysig_dipl:681#tum canis abscondit totius corporis ignis
urn:cts:latinLit:phi0881.phi003.breysig_dipl:682#et latet Orion et semper tutus in undis
